<h1 class="mb-4"><%= t('.header') %></h1>

<p><%= link_to t('.link_add'), new_admin_test_path, class: 'btn btn-primary', role: 'button' %></p>

<table class="table table-striped mt-4">
  <thead class="thead-light">
    <tr>
      <th scope="col">id</th>
      <th scope="col"><%= t('.title') %></th>
      <th scope="col"><%= t('.level') %></th>
      <th scope="col"><%= t('.questions') %></th>
      <th colspan="3" scope="col"><%= t('.actions') %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @tests %>
  </tbody>
</table>