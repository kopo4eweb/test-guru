<%= form_with url: sessions_path, local: true do |form| %>
  <%= output_flash(:alert) %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </p>

  <p><%= form.submit 'Enter' %></p>
<% end %>